CREATE TABLE proveedores (
    id uuid PRIMARY KEY,
    name VARCHAR(255) NOT NULL,
    ruc VARCHAR(13) UNIQUE NOT NULL,
    direccion TEXT NOT NULL,
    estado BOOLEAN DEFAULT TRUE
);
drop table proveedores;

select * from proveedores
